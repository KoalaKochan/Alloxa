# Основное
PRIVATE_KEY=your_private_key_here  # Приватный ключ кошелька в base58 формате
RPC_ENDPOINT=https://api.mainnet-beta.solana.com  # RPC эндпоинт для запросов к Solana
RPC_WEBSOCKET_ENDPOINT=wss://api.mainnet-beta.solana.com  # WebSocket эндпоинт для подписок

# Бот
MAX_TOKENS_AT_THE_TIME=3  # Максимальное количество токенов для одновременной торговли
BACKFILL_SLOTS=50  # Количество слотов для backfill при старте (50 слотов)
FILTER_CHECK_DURATION=50000  # Длительность проверки фильтров в мс (50 сек)
FILTER_CHECK_INTERVAL=3000  # Интервал между проверками фильтров в мс (3 сек)
CONSECUTIVE_FILTER_MATCHES=2  # Количество последовательных совпадений фильтров для покупки

# Transaction Executor
TRANSACTION_EXECUTOR=default  # Тип executor (default/warp/jito)
CUSTOM_FEE=0.001  # Комиссия для warp/jito в SOL

# Фильтры
MIN_POOL_SIZE=80  # Минимальный размер пула в SOL (80 SOL)
MAX_POOL_SIZE=500  # Максимальный размер пула в SOL (500 SOL)
BUY_AMOUNT=0.1  # Сумма покупки в SOL
POOL_MAX_AGE_MS=86400000  # Максимальный возраст пула в мс (24 часа)
HOLDERS_TOP1_MAX_RATIO=0.2  # Максимальная доля топ-1 держателя (20%)
HOLDERS_TOP5_MAX_RATIO=0.35  # Максимальная доля топ-5 держателей (35%)
MIN_SOCIAL_LINKS=1  # Минимальное количество социальных ссылок

# LP защита
REQUIRE_LP_PROTECTION=false  # Требовать защиту LP токенов

# Jupiter
BUY_SLIPPAGE_BPS=300  # Слиппаж для покупки в базисных пунктах (3%)

# Автопродажа
AUTO_SELL=true  # Включить автоматическую продажу
MAX_SELL_RETRIES=5  # Максимальное количество попыток продажи
AUTO_SELL_DELAY=0  # Задержка перед автопродажей в мс
PRICE_CHECK_INTERVAL=2000  # Интервал проверки цены в мс (2 сек)
PRICE_CHECK_DURATION=200000  # Длительность проверки цены в мс (200 сек)

# TP/SL настройки
TAKE_PROFIT=50  # Take Profit в процентах (50% прибыли)
STOP_LOSS=30  # Stop Loss в процентах (30% убытка)
TRAILING_STOP_LOSS=false  # Включить трейлинг стоп-лосс
SKIP_SELLING_IF_LOST_MORE_THAN=0  # Пропустить продажу если потеряно больше % (0 = отключено)
SELL_SLIPPAGE_BPS=300  # Слиппаж для продажи в базисных пунктах (3%)

# Торговля
UNIT_LIMIT=200000  # Лимит единиц для покупки (в минимальных единицах токена)
UNIT_PRICE=0.1  # Цена за единицу в микро-SOL (0.001 SOL)
MAX_BUY_RETRIES=3  # Максимальное количество попыток покупки
AUTO_BUY_DELAY=0  # Задержка перед покупкой в мс



